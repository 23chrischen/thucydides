<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <title>Home</title>
    <style type="text/css">
        <!--
        @import url("css/core.css");
        -->
    </style>
    <link href="css/core.css" rel="stylesheet" type="text/css"/>
    <style type="text/css">a:link {
        text-decoration: none;
    }

    a:visited {
        text-decoration: none;
    }

    a:hover {
        text-decoration: none;
    }

    a:active {
        text-decoration: none;
    }
    </style>
</head>

<body>
<div id="topheader">
    <div id="topbanner">
        <div id="menu">
            <table border="0">
                <tr>
                    <td><a href="home.html"><img src="images/menu_h.png" width="105" height="28" border="0"/></a></td>
                    <td><a href="features.html"><img src="images/menu_f.png" width="105" height="28" border="0"/></a>
                    </td>
                    <td><a href="stories.html"><img src="images/menu_s.png" width="105" height="28" border="0"/></a>
                    </td>
                </tr>
            </table>
        </div>
        <div id="logo"><a href="Home.html"><img src="images/logo.jpg" border="0"/></a></div>
    </div>
</div>

<div class="middlecontent">
    <div id="contenttop">
        <div class="leftbg"></div>
        <div class="middlebg">
            <div style="height:30px;"><span class="bluetext"><a href="Home.html" class="bluetext">Home</a></span> /
            </div>
        </div>
        <div class="rightbg"></div>
    </div>
    <div class="clr"></div>

    <!--/* starts secont table*/-->
    <div id="contentbody">
        <div class="titlebar">
            <div class="leftbgm"></div>
            <div class="middlebgm"><span class="orangetext"><a href="stories.html">Stories</a></span></div>
            <div class="rightbgm"></div>
        </div>
    </div>
    <div class="clr"></div>
    <div id="beforetable"></div>
    <div id="contenttilttle">
        <div class="topb"><img src="images/topm.jpg"/></div>
        <div class="middlb">
            <div class="table">
                <div class="middlb">
                    <div class="table">
                        <div class="toptableraw">
                            <table width="980" height="50" border="0">
                                <tr>
                                    <td width="122">&nbsp;</td>
                                    <td width="198" class="greentext">Stories</td>
                                    <td width="198" class="greentext">Test Cases</td>
                                    <td width="198" class="greentext">Succeeded</td>
                                    <td width="198" class="greentext">Failed</td>
                                    <td width="242" class="greentext">Pending</td>
                                </tr>
                            </table>
                        </div>
                        <div class="tableraw">
                            <table border="0" width="980" height="40">
                                <tr>
                                    <td width="122">&nbsp;</td>
                                    <td width="198" class="bluetext">$stories.storyCount</td>
                                    <td width="198" class="bluetext">$stories.acceptanceTestCount</td>
                                    <td width="242"><span class="lightgreentext"><img src="images/tick.png" width="16"
                                                                                      height="16"
                                                                                      style="padding:00px 3px 00px 00px;"/>$stories.successCount</span>
                                    </td>
                                    <td width="198"><span class="redtext"><span class="lightgreentext"><img
                                            src="images/cross.png" width="16" height="16"
                                            style="padding:00px 3px 00px 00px;"/></span>$stories.failureCount</span>
                                    </td>
                                    <td width="242"><span class="lightgreentext"><img src="images/pending.png" width="16"
                                                                                      height="16"
                                                                                      style="padding:00px 3px 00px 00px;"/>$stories.pendingCount</span>
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="bottomb"><img src="images/bottomm.jpg"/></div>
    </div>
</div>
<div id="beforefooter"></div>
<div id="bottomfooter"></div>

</body>
</html>
